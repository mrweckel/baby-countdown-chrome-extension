{
  "manifest_version": 2,

  "name": "Baby Countdown",
  "description": "Countdown the seconds until your baby is born",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Baby Countdown"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "chrome_url_overrides" : {
    "newtab": "countdown.html"
  }
}